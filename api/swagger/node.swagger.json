        },
        "devAddr": {
          "type": "string",
          "description": "Hex encoded DevAddr."
        },
        "appSKey": {
          "type": "string",
          "description": "Hex encoded AppSKey."
        },
        "nwkSKey": {
          "type": "string",
          "description": "Hex encoded NwkSKey."
